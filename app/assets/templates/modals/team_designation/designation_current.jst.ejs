<ul class="current-member-items">
  <<% users.each( function(user) { %>
    <li class="current-member-item">
      <figure class="avatar">
        <img src="<%= user.escape('avatar') %>"
             alt="<%= Trak.Utils.imageName(user) %>" />
      </figure>
      <div class="user-name">
        <%= user.escape('name') %>
      </div>
    </li>
  <% }) %>
</ul>
