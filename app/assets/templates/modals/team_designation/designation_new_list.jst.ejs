<% users.each( function(user) { %>
  <li class="search-result-item" data-user-id="<%= user.id %>">
    <figure class="avatar">
      <img src="<%= user.escape('avatar') %>"
           alt="<%= Trak.Utils.imageName(user) %>" />
    </figure>

    <div class="user-name">
      <%= user.escape('name') %>
    </div>

    <div class="user-email">
      <%= user.escape('email') %>
    </div>
  </li>
<% }) %>
