<% if (project) { %>
  <h3 class="project-codename">
    <%= project.escape('code_name') %>
  </h3>
  <h4 class="project-subheading">
    <%= currTeamMoniker %>
  </h4>
<% } else { %>
  <h3 class="project-codename">
    <%= Trak.currentUser.escape('name') %>'s <%= currTeamMoniker %> Tasks
  </h3>
  <h4 class="project-subheading">
    All current team tasks assigned to you
  </h4>
<% } %>
